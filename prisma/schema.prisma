generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model category {
  id   Int @id @default(autoincrement())
  name String

  
}

model products {
  name       String
  barcode    Int  
  price      Decimal
  created_at DateTime @default(now())
  id         Int      @id @default(autoincrement())
}